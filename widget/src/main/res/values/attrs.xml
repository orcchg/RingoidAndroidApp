<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Main">
        <attr name="refColorAccent" format="reference"/>
        <attr name="refColorPrimary" format="reference"/>
        <attr name="refColorPrimaryDark" format="reference"/>
        <attr name="refColorBottomBarIcon" format="reference"/>
        <attr name="refColorBottomBarIconSelected" format="reference"/>
        <attr name="refColorUnderscore" format="reference"/>
        <attr name="refTextColorPrimary" format="reference"/>
        <attr name="refTextColorSecondary" format="reference"/>
        <attr name="iconItemView" format="reference"/>
    </declare-styleable>

    <!-- Widget -->
    <declare-styleable name="ForegroundImageView">
        <attr name="xForeground" format="reference"/>
    </declare-styleable>

    <declare-styleable name="IconItemView">
        <attr name="icon_item_color" format="reference"/>
        <attr name="icon_item_icon" format="reference"/>
        <attr name="icon_item_hide_icon" format="boolean"/>
        <attr name="icon_item_text" format="reference|string"/>
        <attr name="refItemViewArrow" format="reference"/>
    </declare-styleable>
</resources>
